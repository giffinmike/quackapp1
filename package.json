{
  "name": "quackapp1",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:5001",
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tiptap/extension-collaboration": "^2.4.0",
    "@tiptap/extension-collaboration-cursor": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "framer-motion": "^11.2.13",
    "mongoose": "^8.4.5",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.24.1",
    "react-scripts": "5.0.1",
    "tsconfig-paths": "^4.2.0",
    "web-vitals": "^2.1.4",
    "y-prosemirror": "^1.2.9",
    "y-webrtc": "^10.3.0",
    "y-websocket": "^2.0.3",
    "yjs": "^13.6.18"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --watch backend --exec node --loader ts-node/esm backend/server.ts",
    "client": "PORT=3000 react-scripts start",
    "build": "npm run build-client && npm run build-server",
    "build-client": "react-scripts build --max-old-space-size=512",
    "build-server": "tsc --project tsconfig.build.json",
    "serve": "concurrently \"serve -s build -l 3000\" \"node build/server.js\"",
    "serve-local": "concurrently \"serve -s build -l 3000\" \"node build/server.js\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "npm run build",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.14.9",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "workbox-webpack-plugin": "^7.1.0"
  }
}
